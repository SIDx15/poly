<div class="buttons-wrapper">
  <a href="/" class="logo">MyPaste</a>
  <p class="credit">Made by Siddharth Das</p>
  <div class="buttons">
    <a href="/new" class="btn">New</a>
    <% if (locals.canSave) { %>
      <button class="btn" type="submit">Save</button>
    <% } %>
    <a href="/all" class="btn">Display</a>
    <a href="/change" class="btn">Change</a>
    <a href="/log" class="btn">Logs</a>
    <% if (locals.id != null) { %>
      <a href="/<%= locals.id %>/duplicate" class="btn">Duplicate</a>
    <% } %>
  </div>
</div>